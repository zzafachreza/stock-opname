<?xml version='1.0' encoding='utf-8'?>
<widget id="com.setiabudhi.so" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Setiabuhi Stock Opname</name>
    <description>
        Aplikasi ini dibuat khusus Setiabudhi Supermarket dalam kegiatan Stock Opname
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Fachreza Maulana
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="StatusBarOverlayWebView" value="true"/>
        <preference name="StatusBarBackgroundColor" value="#FC0800"/>
        <preference name="StatusBarStyle" value="lightcontent"/>
        <preference name="StausBarDefaultScrollToTop" value="false"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
